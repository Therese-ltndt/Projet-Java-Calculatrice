package calculette_code;

import java.util.Stack;

public class Pile extends java.util.Stack<Double>{
	
	
	public void push(double d) {
		super.push(d);
	}
	
	public Double pop() {
		return super.pop();
		}
		
	
	public boolean empty() {
		return super.empty();
	}
	
	public void clear() {
		super.clear();
	}
}
