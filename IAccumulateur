package calculette_code;

public interface IAccumulateur {

	public void push();
	
	public double drop();
	
	public void swap();
	
	public void add();
	
	public void sub();
	
	public void mult();
	
	public void div();
	
	public void neg();
	
	public void backspace();
	
	public String accumuler(String character);
	
	public void reset();
	
	public double stringToDouble(String s);
	
}
